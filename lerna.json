{
  "ignoreChanges": ["**/.github/**", "**/*.md"],
  "npmClient": "yarn",
  "packages": ["packages/*"],
  "command": {
    "version": {
      "allowBranch": ["master"],
      "message": "chore(release): publish [skip ci]"
    }
  },
  "version": "independent",
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}

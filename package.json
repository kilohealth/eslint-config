{
  "name": "@kilohealth/eslint-config",
  "private": true,
  "keywords": [
    "react-native",
    "eslint",
    "eslintconfig",
    "prettier",
    "react",
    "next",
    "node",
    "redux-saga"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "sort-all-package-json": "yarn sort-package-json \"package.json\" \"packages/*/package.json\"",
    "version": "yarn install && git stage yarn.lock"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "*.{js,json,md,yml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lerna": "^7.3.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "redux-saga": "^1.2.3",
    "sort-package-json": "^2.6.0"
  },
  "packageManager": "yarn@3.2.2"
}
